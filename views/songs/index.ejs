<%- include('../partials/header') %>

    <p>Recommended songs for your hiking playlist</p>
    <a href="/playlist/new">Click here to share your favorite songs</a>
    <table id="list">
        <thead>
            <tr id="song">
                <th>Artist</th>
                <th>Song</th>
            </tr>
        </thead>
        <table id="list">
            <tbody>
                <% songs.forEach(function(t) { %>
                    <tr>
                        <td>
                            <%= t.artist %>
                        </td>
                        <td>
                            -
                        </td>

                        <td>
                            <em>
                                <%= t.songName %>
                            </em>
                        </td>

                    </tr>
                    <% }); %>
            </tbody>
        </table>

        <%- include('../partials/footer') %>